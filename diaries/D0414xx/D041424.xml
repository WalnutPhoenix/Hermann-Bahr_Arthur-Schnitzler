        <div type="writingSession" n="1">
             <p output="true" ><date when="19030210" n="01">8/2–10/2</date>
            </p>
            <p output="true" >Dictiert, was mir aber noch recht schwer fällt.</p>
            <p><persName key="A002723">Hans Schlesinger</persName>, <persName key="A003208">Pohl-Meiser</persName>,
                        <persName key="A002064">Trebitsch</persName>, <persName key="A002000">Burckhard</persName>,
                        <persName key="A002774">Klimt</persName>, <persName key="A003111">Moser</persName>,
                        <persName key="A003100">Moll</persName>, Prof. <persName key="A003215">Schiff</persName>, Papa
                        <persName key="A002139">Hofmannsthal</persName>, <persName key="A002001">Arthur Schnitzler</persName>,
                        <persName key="A003216">Frieberger</persName>, <persName key="A002067">Wilhelm Singer</persName>,
                        <persName key="A002669">Epstein</persName>.</p>
            <p><persName key="A002205">Hebbels</persName><space unit="chars" quantity="1"/><workName key="A020346">Tagebücher</workName> u. <workName key="A020424">Briefwechsel</workName> beendet.</p>
            <p><persName key="A003217">Marion Crawford</persName>: <workName key="A020553">Cecilia</workName>.</p>
        </div>
