   <text type="diaryDay">
      <body>
        <div type="writingSession" n="1">
             <p><date when="19030102" n="01"/>
               <placeName key="A000537">Frick</placeName>: <persName key="A003194">
                  <workName key="A020596">Leonardo</workName>
               </persName> – <persName key="A003282">Knackfuß</persName>
            </p>
                <p><persName key="A002393">Heymann</persName>: <workName key="A020143">Kritik der Kritik</workName> 
            </p>
                <p>Ob <hi rend="underline" n="1">
                  <persName key="A002398">Bell</persName>
               </hi> oder <workName key="A020597">Masters in Art <persName key="A003194">Leonardo</persName>
               </workName> 
            </p>
                <p><persName key="A003281">Zillner</persName> »<workName key="A020595">Gesch der Stadt <placeName key="A000230"><settlement>Salzburg</settlement></placeName>
               </workName>« 1890 u 95 </p>
            <p><table cols="2" rows="5">
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1">Morgen: </cell>
                    <cell role="data" rows="1" cols="1">
                        <persName key="A002067">Singer</persName> oder <persName key="A003280">Skrein</persName>
                     </cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">
                        <persName key="A002001">Schnitzler</persName>
                     </cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">Redactionstrinkgelder</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">Briefpapier</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                      <cell role="data" rows="1" cols="1">Leiter neben <placeName key="A000538">Hotel de France</placeName> (2 Meter 70, <hyphenation alt="}\pend\pstart\noindent\makebox[\the\longeste][l]{}\makebox[\the\longestz][l]{da Kasten 2 M 30 ist)">da Kasten 2 M 30 ist)</hyphenation></cell>
                  </row>
               </table>
            </p>

            <p><table cols="3" rows="16">
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1">Einnahmen</cell>
                    <cell role="data" rows="1" cols="1">Von oder für</cell>
                    <cell role="data" rows="1" cols="1"><anchor type="textConst" xml:id="TD450-1" >Ausgaben</anchor>
                     </cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">Nach Bezalg von 500 Kr an <persName key="A002186">Rosa</persName> bleiben mir</cell>
                    <cell role="data" rows="1" cols="1"/>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1">600 Kr.</cell>
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1"/>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">
                        <persName key="A004378">Brieftr</persName>
                     </cell>
                    <cell role="data" rows="1" cols="1">10 Kr</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">
                        <persName key="A004379">Gemeindediener</persName>
                     </cell>
                    <cell role="data" rows="1" cols="1">2 Kr</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">für Hefte</cell>
                    <cell role="data" rows="1" cols="1">4 Kr</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">Commission</cell>
                    <cell role="data" rows="1" cols="1">1 Kr</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">Apotheke</cell>
                    <cell role="data" rows="1" cols="1">2 68 h</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">Trinkgelder für Diener des <orgName  key="A080148">N. W.</orgName> u. der <orgName  key="A080153">Ö. V.</orgName>
                     </cell>
                    <cell role="data" rows="1" cols="1">30 Kr.</cell>
                  </row>
               
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">
                        <persName key="A003283">Ferdinand</persName> Comission</cell>
                    <cell role="data" rows="1" cols="1">2</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">Wagen</cell>
                    <cell role="data" rows="1" cols="1">3 30</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">
                        <persName key="A004378">Briefträger</persName>
                     </cell>
                    <cell role="data" rows="1" cols="1">3</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">Feuerwehrball</cell>
                    <cell role="data" rows="1" cols="1">4</cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1">39 Kr</cell>
                    <cell role="data" rows="1" cols="1">vom <orgName  key="A080598">Rubinverlag</orgName> am 4/1 erhalten</cell>
                    <cell role="data" rows="1" cols="1"/>
                  </row>
                
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">
                        <hi rend="overline">61 98</hi>
                     </cell>
                  </row>
                  <row role="data" rows="1" cols="1">
                    <cell role="data" rows="1" cols="1"/>
                    <cell role="data" rows="1" cols="1">An <persName key="A003179">Ludwig</persName> noch am 31. Dez. bezalt</cell>
                    <cell role="data" rows="1" cols="1">30 Kr</cell>
                  </row>
               </table>
            </p>
        </div>
      </body>
   </text>
